{
    "name": "Azure OpenAI GPT4o",
    "connector_type": "azure-openai-connector",
    "uri": "https://<Azure OpenAI Name>.openai.azure.com",
    "token": "{your token here}",
    "max_calls_per_second": 1,
    "max_concurrency": 1,
    "model": "gpt-4o",
    "params": {
        "timeout": 300,
        "max_attempts": 3,
        "temperature": 0.5
    }
}